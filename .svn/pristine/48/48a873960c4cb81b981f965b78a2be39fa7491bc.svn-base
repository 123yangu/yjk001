<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/color_yeehealth_main"
    android:orientation="vertical" >

    <include layout="@layout/layout_title" />

    <RelativeLayout
        style="@style/rectangular_four_edges_spacing"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/white" >

        <com.yeehealth.yjk.widget.circleimg.CircleImageView
            android:id="@+id/user_image_header"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/default_user_gray_image" />
    </RelativeLayout>

</LinearLayout>